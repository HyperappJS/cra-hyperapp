{
  "name": "cra-hyperapp",
  "version": "0.0.5",
  "description": "Hyperapp expansion pack for create-react-app",
  "main": "src/index.js",
  "bin": {
    "hyperapp-scripts": "./bin/index.js"
  },
  "scripts": {
    "format": "npx prettier --write **/*.js",
    "format:check": "npx prettier --list-different **/*.js",
    "release": "npm run format:check && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "dependencies": {
    "react-scripts": "=1.1.0"
  },
  "peerDependencies": {
    "hyperapp": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okwolf/cra-hyperapp.git"
  },
  "author": "Wolfgang Wedemeyer <wolf@okwolf.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okwolf/cra-hyperapp/issues"
  },
  "homepage": "https://github.com/okwolf/cra-hyperapp"
}
